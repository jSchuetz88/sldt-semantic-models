@prefix samm: <urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#> .
@prefix samm-c: <urn:samm:org.eclipse.esmf.samm:characteristic:2.1.0#> .
@prefix samm-e: <urn:samm:org.eclipse.esmf.samm:entity:2.1.0#> .
@prefix unit: <urn:samm:org.eclipse.esmf.samm:unit:2.1.0#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <urn:samm:io.catenax.warranty_claim_request_verification:1.0.0#> .
@prefix ext-bpn: <urn:samm:io.catenax.shared.business_partner_number:2.0.0#> .
@prefix ext-uuid: <urn:samm:io.catenax.shared.uuid:2.0.0#>.

:WarrantyClaimRequestVerification a samm:Aspect ;
   samm:preferredName "Warranty Claim Request"@en ;
   samm:description "XXX "@en ;
   samm:properties ( :claimId :qualityTaskId :cxBPN :vehicleId :anonymizedVIN :billingId :billingDate :agreedTF :debitValueJustified :billedCurrency :claimPlausible :claimImplausible :billStatus :statementDate :statementReason  ) ;
   samm:operations ( ) ;
   samm:events ( ) .

# Do we need an individual trait? Is it actually BPNxclaimId?
:claimId a samm:Property ;
   samm:preferredName "Claim Identifier"@en ;
   samm:description "Each claim from this data provider has a unique claim ID.If you combine the BPNL of the data provider with this claim ID you get a unique claim ID in the data space."@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "a214-13d6" .

:qualityTaskId a samm:Property ;
   samm:preferredName "Quality Task Identifier"@en ;
   samm:description "A unique quality task identifier within this data-space."@en ;
   samm:characteristic ext-uuid:UuidV4Trait .

# BPNL, BPNA or BPNS?
:cxBPN a samm:Property ;
   samm:preferredName "Business Partner Number"@en ;
   samm:description "A unique plant identifier according BPNS definition. This property is optional as of now not all plants have assigned BPNS identifiers."@en ;
   samm:characteristic ext-bpn:BpnlCharacteristic ;
   samm:exampleValue "BPNL 1234 5678 90ZZ" .

:vehicleId a samm:Property ;
   samm:preferredName "Vehicle Identifier"@en ;
   samm:description "A unique identifier for this vehicle in data-space. This property is optional as of now not all vehicles have this identifier assigned."@en ;
   samm:characteristic ext-uuid:UuidV4Trait .

# Freetext?
:anonymizedVIN a samm:Property ;
   samm:preferredName "XXX"@en ;
   samm:description "Vehicle manufacturer-specific anonymized VIN, which is still a unique identifier within this data provider."@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "3747429FGH382923974682" .

:billingId a samm:Property ;
   samm:preferredName "XXX"@en ;
   samm:description "XXX"@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "XXX" .

:billingDate a samm:Property ;
   samm:preferredName "XXX"@en ;
   samm:description "XXX"@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "XXX" .

:agreedTF a samm:Property ;
   samm:preferredName "XXX"@en ;
   samm:description "TF which was agreed upon"@en ;
   samm:characteristic [
     a samm:Characteristic ;
      samm:dataType xsd:positiveInteger
   ] ;
   samm:exampleValue 50 .

:debitValueJustified a samm:Property ;
   samm:preferredName "XXX"@en ;
   samm:description "Check between billedTF & agreedTF; other plausibility checks"@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "XXX" .

:billedCurrency a samm:Property ;
   samm:preferredName "XXX"@en ;
   samm:description "Billing currency"@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "EUR" .

:claimPlausible a samm:Property ;
   samm:preferredName "XXX"@en ;
   samm:description "Check between billedTF & agreedTF"@en ;
   samm:characteristic :CurrencyCharacteristic ;
   samm:exampleValue 500 .

:claimImplausible a samm:Property ;
   samm:preferredName "XXX"@en ;
   samm:description "Check between billedTF & agreedTF"@en ;
   samm:characteristic :CurrencyCharacteristic ;
   samm:exampleValue 500 .

:billStatus a samm:Property ;
   samm:preferredName "XXX"@en ;
   samm:description "Status of the bill based on claim_request_verification assessment (approved / disputed / â€¦)"@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "XXX" .

:statementDate a samm:Property ;
   samm:preferredName "XXX"@en ;
   samm:description "Due date for the claim_request_verification to approve / dispute the bill"@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "XXX" .

:statementReason a samm:Property ;
   samm:preferredName "XXX"@en ;
   samm:description "Reason give by the claim_request_verification, why the bill / claim was disputed"@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "TF too high" .

:BillableMinutesCharacteristic a samm:Characteristic ;
      samm:dataType xsd:positiveInteger . 

:CurrencyCharacteristic a samm:Characteristic ;
      samm:dataType xsd:positiveFloat . 