@prefix : <urn:samm:io.catenax.warranty_claim_request:1.0.0#> .
@prefix ext-bpn: <urn:samm:io.catenax.shared.business_partner_number:2.0.0#> .
@prefix ext-uuid: <urn:samm:io.catenax.shared.uuid:2.0.0#>.

:WarrantyClaimRequest a samm:Aspect ;
   samm:preferredName "Warranty Claim Request"@en ;
   samm:description "The focus of the data model is on standardizing the exchange of warranty information within the Catena-X network. This issue primarily affects suppliers who do not receive quality data from their customers (OEMs) and do not exchange data with their own suppliers (Tier 2 - n). These suppliers receive warranty claims from their customers (OEMs) at regular or irregular intervals. Due to a lack of “real” quality data, these claims are used to derive a quality analysis of the faulty components. "@en ;
   samm:properties ( :claimId :qualityTaskId :cxBPN :vehicleId :anonymizedVIN :billingId :billingDate :repairIndicator :recoveryType :warrantyType :recoveryGroupCode :recoveryGroupName :billedCurrency :billedAmount :billedTF :repairCostsTF100 :materialCosts :laborCosts :otherCosts :hourlyRate :globalLaborCode :globalLaborCodeDesc :baseLaborHours :otherLaborHours :supplementalLaborHours :diagnosticLaborHours :totalHours ) ;
   samm:operations ( ) ;
   samm:events ( ) .

# Do we need an individual trait? Is it actually BPNxclaimId?
:claimId a samm:Property ;
   samm:preferredName "Claim Identifier"@en ;
   samm:description "Each claim from this data provider has a unique claim ID. If you combine the BPNL of the data provider with this claim ID you get a unique claim ID in the data space."@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "a214-13d6" .

:qualityTaskId a samm:Property ;
   samm:preferredName "Quality Task Identifier"@en ;
   samm:description "A unique quality task identifier within this data space."@en ;
   samm:characteristic ext-uuid:UuidV4Trait .

:cxBPNS a samm:Property ;
   samm:preferredName "Business Partner Number"@en ;
   samm:description "A unique plant identifier according to BPNS definition. This property is optional as of now not all plants have assigned BPNS identifiers."@en ;
   samm:characteristic ext-bpn:BpnlCharacteristic ;
   samm:exampleValue "BPNS 1234 5678 90ZZ" .

:vehicleId a samm:Property ;
   samm:preferredName "Vehicle Identifier"@en ;
   samm:description "A unique identifier for this vehicle in data-space. This property is optional as of now not all vehicles have this identifier assigned."@en ;
   samm:characteristic ext-uuid:UuidV4Trait .

# Todo Johann: Prüfen ob Shared Model existiert
:anonymizedVIN a samm:Property ;
   samm:preferredName "XXX"@en ;
   samm:description "Vehicle manufacturer-specific anonymized VIN, which is still a unique identifier within this data provider."@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "3747429FGH382923974682" .

:billingId a samm:Property ;
   samm:preferredName "XXX"@en ;
   samm:description "Billing ID is unique for each OEM"@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "B123456789" .

# Usually not how it is intended. If all formats are possible, it has to be a free-text. With a free-text, you could add anything here.
# Todo Johann: Check for shared model for date 
:billingDate a samm:Property ;
   samm:preferredName "Billing date"@en ;
   samm:description "References the date when the claim was billed (all date formats possible)"@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "XXX" .

# No standardized list?
:repairIndicator a samm:Property ;
   samm:preferredName "Repair Indicator"@en ; 
   samm:description "Type of activitiy; e.g., regular warranty, TSB work, service campaign, …"@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "Regular" .

:recoveryType a samm:Property ;
   samm:preferredName "Recovery type"@en ;
   samm:description "OEM to Supplier (Standard / Replacement / …)"@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "Standard" .

:warrantyType a samm:Property ;
   samm:preferredName "Warranty type"@en ;
   samm:description "OEM internal (Vehicle warranty / Spare part warranty / Goodwill …)"@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "Vehicle warranty" .

:recoveryGroupCode a samm:Property ;
   samm:preferredName "Recovery group code"@en ;
   samm:description "Code for a specific part family / technically similar product groups"@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "A258" .

:recoveryGroupName a samm:Property ;
   samm:preferredName "Recovery group name"@en ;
   samm:description "Name for a specific part family / technically similar product groups"@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "Part family A" .

